    "jest-cli": "^20.0.1",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^4.1.1",
    "react-test-renderer": "^15.4.2",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "1.*",
